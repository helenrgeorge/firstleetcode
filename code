/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
let twoSum = function(nums, target) {
    for (let a=0; a<nums.length;a++){
    for (let b=1; b<nums.length + 1; b++) {
        let result = nums[a] + nums[b]
        if (result == target) {
            let results = [a, b]
            return results
        }
        
    }

    }    
    
    
};
